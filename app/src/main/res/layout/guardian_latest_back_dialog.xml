<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/transparent"
    android:orientation="vertical"
    android:paddingStart="@dimen/dimen_30"
    android:paddingEnd="@dimen/dimen_30"
    tools:context="com.cqkct.FunKidII.Ui.Activity.MoreFunction.AmGuardTimeActivity">

    <include layout="@layout/normal_dialog_arched_top_title_layout" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/main_background_color"
        android:orientation="vertical">

        <View
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:background="@color/main_background_color" />

        <include layout="@layout/select_start_time" />

        <View
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:background="@color/main_background_color" />
    </LinearLayout>

    <include layout="@layout/ok_and_cancel" />
</LinearLayout>

